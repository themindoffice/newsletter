{
	"name": "themindoffice/newsletter",
	"description": "Add newsletter addon to Iris",
	"license": "MIT",
	"type": "library",
	"version": "1.0.0",
	"authors": [{
		"name": "The MindOffice",
		"email": "development@themindoffice.nl"
	}],
	"autoload": {
		"psr-4": {
			"Modules\\Addons\\Newsletter\\": "src/Newsletter"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php -r \"require 'vendor/themindoffice/newsletter/src/Newsletter/install/run.php';\""
		],
		"post-update-cmd": [
		  	"touch hello.txt",
			"php -r \"require 'vendor/themindoffice/newsletter/src/Newsletter/install/run.php';\""
		]
	}
}
