{
	"name": "themindoffice/newsletter",
	"description": "Add newsletter addon to Iris",
	"license": "MIT",
	"type": "library",
	"version": "1.0.0",
	"authors": [{
		"name": "The MindOffice",
		"email": "development@themindoffice.nl"
	}],
	"require": {
		"slowprog/composer-copy-file": "~0.3"
	},
	"require-dev": {
		"slowprog/composer-copy-file": "~0.3"
	},
	"autoload": {
		"psr-4": {
			"Modules\\Addons\\Newsletter\\": "src/Newsletter"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"SlowProg\\CopyFile\\ScriptHandler::copy",
			"# <info>For your info 1</info>",
			"# <comment>A comment 1</comment>",
			"# <error>Error!!!! 1</error>",
			"# <href=https://symfony.com>Symfony Homepage 1</>"
		],
		"post-update-cmd": [
			"SlowProg\\CopyFile\\ScriptHandler::copy",
			"# <info>For your info 2</info>",
			"# <comment>A comment 2</comment>",
			"# <error>Error!!!! 2</error>",
			"# <href=https://symfony.com>Symfony Homepage 2</>"
		]
	},
	"extra": {
		"copy-file": {
			"vendor/themindoffice/newsletter/src": "modules/Addons"
		},
		"copy-file-dev": {
			"vendor/themindoffice/newsletter/src": "modules/Addons"
		}
	}
}
